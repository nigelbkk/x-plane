{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to X-Plane",
            "type": "cppdbg",
            "request": "attach",
            "program": "/media/nigel/XP12/ubuntu/X-Plane-12/X-Plane-x86_64",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "cwd": "${workspaceFolder}",
            "externalConsole": false
        }
    ]
}
